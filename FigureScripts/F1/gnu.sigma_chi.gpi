set t po eps enh color 30
set output 'sigma_chi.eps'

set size 1, 1
set tics scale 2
set border lw 2.5
set ytics 0.1 
set xlab 'Standard deviation {/Symbol s}' 
set ylab 'Freaction of cyclic dominance {/Symbol \143}'
