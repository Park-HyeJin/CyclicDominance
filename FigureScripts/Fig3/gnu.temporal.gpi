set output 'temporal.pdf'
set xlab "Mutation event time"
set ylab ""

Nsur = 498
p 'res/d0.005_mu1e-05_n_probs.txt' u 1:($2/Nsur) w l t 'n'
p 'res/d0.005_mu1e-05_n_probs.txt' u 1:($3/Nsur) w l t 'p(t14)'\
, 'res/d0.005_mu1e-05_n_probs.txt' u 1:($4/Nsur) w l t 'p(t15)'
p 'res/d0.005_mu1e-05_n_probs.txt' u 1:5 w lp t 'chi'

